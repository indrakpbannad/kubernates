pipeline {
  agent any	
  stages {
    stage('Cloning Git') {
      steps {
        checkout scm
      }
	}
	
	 stage('Deploying App to Kubernetes') {
      steps {
        script {
          kubernetesDeploy(configs: "k8s-deployment-service.yaml", kubeconfigId: "k8s")
        }
      }
    }
	
  }
} 
